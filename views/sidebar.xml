<odoo>
    <template id="portal_my_home_menu_visa" name="Portal My Home Menu - Visa" inherit_id="portal.portal_my_home" priority="40">
        <xpath expr="//div[hasclass('o_portal_docs')]" position="inside">
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Visa Dashboard</t>
                <t t-set="url" t-value="'/my/visa/dashboard'"/>
                <t t-set="placeholder_count" t-value="'visa_dashboard'"/>
            </t>
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Students</t>
                <t t-set="url" t-value="'/my/visa/students'"/>
                <t t-set="placeholder_count" t-value="'visa_students'"/>
            </t>
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Applications</t>
                <t t-set="url" t-value="'/my/visa/applications'"/>
                <t t-set="placeholder_count" t-value="'visa_applications'"/>
            </t>
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Documents</t>
                <t t-set="url" t-value="'/my/visa/documents'"/>
                <t t-set="placeholder_count" t-value="'visa_documents'"/>
            </t>
            <t t-call="portal.portal_docs_entry">
                <t t-set="title">Payments</t>
                <t t-set="url" t-value="'/my/visa/payments'"/>
                <t t-set="placeholder_count" t-value="'visa_payments'"/>
            </t>
        </xpath>
    </template>
</odoo>